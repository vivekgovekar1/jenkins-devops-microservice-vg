pipeline {
	agent any  
		stages {
			stage("Build") {
				steps {
					echo "Build -1"
					ecno  "Build -2"
				}
			}
			stage("Test") 
				steps {
					echo "Test -1"
					ecno "Test -2"
				}		
			stage("Intgration Test") 
				steps {
					echo "Intgration Test -1"
					ecno "Intgration Test -2"
				}
		}
	}
}
